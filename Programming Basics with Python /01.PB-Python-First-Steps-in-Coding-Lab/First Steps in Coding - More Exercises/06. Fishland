price_skumria = float(input())
price_caca = float(input())
kg_palamud = float(input())
kg_safrid = float(input())
kg_midi = int(input())

price_palamud = kg_palamud * (price_skumria * 1.6)
price_safrid = kg_safrid * (price_caca * 1.8)
price_midi = 7.50 * kg_midi

total_sum = price_palamud + price_safrid + price_midi

print(f'{total_sum:.2f}')
