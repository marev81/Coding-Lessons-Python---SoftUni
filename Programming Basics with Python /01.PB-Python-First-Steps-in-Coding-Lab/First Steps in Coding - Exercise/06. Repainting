nylon = int(input())
paint = int(input())
thinner = int(input())
hours = int(input())
#
price_nylon = 1.50
price_paint = 14.50
price_thinner = 5.00
price_bags = 0.40
#
sum_need_nylon = (nylon+2)*price_nylon
sum_need_paint = (paint+(paint*0.1))*price_paint
sum_need_thinner = thinner * price_thinner
#
material_expenses = sum_need_nylon + sum_need_paint + sum_need_thinner + price_bags
payment_workers = (material_expenses * 0.3) * hours
#
final_price = material_expenses + payment_workers
#
print(final_price)

